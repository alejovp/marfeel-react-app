{
  "name": "webpack-base",
  "version": "1.0.0",
  "description": "webpack project base",
  "private": true,
  "scripts": {
    "build": "npm run pre-build && NODE_ENV=production webpack",
    "local-build": "npm run pre-build && NODE_ENV=local webpack",
    "dev": "npm run local-build && NODE_ENV=local webpack serve",
    "pre-build": "rm -rf public/* && npm run eslint",
    "reinstall": "rm -f package-lock.json && rm -Rf node_modules && npm i",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "eslint": "eslint ./src/ --ext js --cache",
    "eslint-fix": "eslint ./src/ --ext js --fix",
    "watch": "npm run local-build && NODE_ENV=local webpack --watch"
  },
  "author": "Alejandro VÃ¡zquez",
  "license": "MIT",
  "dependencies": {
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-classes": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.1.0",
    "core-js": "3.6.5",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "enzyme-to-json": "3.6.1",
    "eslint": "7.12.1",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-react": "7.21.5",
    "html-webpack-plugin": "5.0.0-alpha.7",
    "jest": "26.6.1",
    "jest-styled-components": "7.0.3",
    "react-test-renderer": "16.14.0",
    "webpack": "5.4.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0"
  }
}
